{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "server/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/api(.*)",
            "dest": "/server/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}